{
  "name": "atom-metaed",
  "main": "./lib/atom-metaed",
  "version": "1.2.1-dev.1",
  "description": "A MetaEd plugin for Atom",
  "keywords": [],
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
  "configSchema": {
    "targetDsVersion": {
      "order": 1,
      "title": "Target Ed-Fi Data Standard Version",
      "description": "Select the target Ed-Fi Data Standard version.",
      "type": "string",
      "default": "2.0.1",
      "enum": [
        {
          "value": "2.0.1",
          "description": "2.0.1"
        },
        {
          "value": "3.0",
          "description": "3.0"
        }
      ]
    },
    "targetOdsApiVersion": {
      "order": 2,
      "title": "Target Ed-Fi ODS/API Version",
      "description": "Select the target Ed-Fi ODS/API version.",
      "type": "string",
      "default": "2.3.1",
      "enum": [
        {
          "value": "2.0",
          "description": "2.0"
        },
        {
          "value": "2.1",
          "description": "2.1"
        },
        {
          "value": "2.2",
          "description": "2.2"
        },
        {
          "value": "2.3",
          "description": "2.3"
        },
        {
          "value": "2.3.1",
          "description": "2.3.1"
        },
        {
          "value": "2.4",
          "description": "2.4"
        },
        {
          "value": "3.0",
          "description": "3.0"
        },
        {
          "value": "3.1",
          "description": "3.1"
        }
      ]
    },
    "edfiOdsApiSourceDirectory": {
      "order": 3,
      "title": "Ed-Fi ODS / API source directory",
      "description": "Full path to root folder for the Ed-Fi ODS / API source code.  The folder this points to should contain Ed-Fi-ODS and Ed-Fi-ODS-Implementation folders.",
      "type": "string",
      "default": ""
    },
    "telemetryConsent": {
      "order": 4,
      "title": "Consent to anonymous usage reporting",
      "description": "Select whether you are willing to submit anonymous usage information to the Ed-Fi Alliance server. Broadly, we send things like performance metrics and exceptions.",
      "type": "string",
      "default": "",
      "enum": [
        {
          "value": "",
          "description": "Please select:"
        },
        {
          "value": "true",
          "description": "I consent."
        },
        {
          "value": "false",
          "description": "I do not consent."
        }
      ]
    },
    "validateOnTheFly": {
      "order": 5,
      "title": "Enable validation as you type (experimental)",
      "description": "Performs validation on the fly (instead of on save).  May introduce a delay in the editor UI.  Requires Atom restart after changing.",
      "type": "boolean",
      "default": true
    },
    "coreMetaEdSourceDirectory": {
      "order": 6,
      "title": "Ed-Fi Data Standard core .metaed directory",
      "description": "Full path to the set of core MetaEd files for the Ed-Fi Data Standard.",
      "type": "string",
      "default": ""
    },
    "cmdFullPath": {
      "order": 7,
      "title": "Full path to Windows command prompt (cmd.exe)",
      "description": "Full path to cmd.exe, used to spawn the MetaEd Console and provide highlighted output.",
      "type": "string",
      "default": "C:\\Windows\\System32\\cmd.exe"
    },
    "alliance": {
      "order": 9,
      "title": "",
      "type": "object",
      "collapsed": true,
      "properties": {
        "allianceMode": {
          "order": 8,
          "title": "Alliance only",
          "description": "For Alliance users only.  Requires Atom restart.",
          "type": "boolean",
          "default": false
        }
      }
    }
  },
  "scripts": {
    "postinstall": "apm link",
    "postuninstall": "apm unlink atom-metaed",
    "build:clean": "rimraf ./build/dist",
    "build:dist": "mkdir .\\build\\dist && babel ./build/build.js -o ./build/dist/build.js --source-maps inline",
    "build": "yarn build:clean && yarn build:dist",
    "ci": "node ./build/dist/build.js",
    "test": "apm test"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "ansi-html": "^0.0.7",
    "atom-message-panel": "^1.3.0",
    "atom-package-deps": "^4.6.0",
    "atom-space-pen-views": "2.2.0",
    "axios": "0.18.0",
    "babel-polyfill": "^6.26.0",
    "ed-fi-model-2.0": "1.0.3",
    "ed-fi-model-3.0": "1.0.0",
    "fs-extra": "^3.0.1",
    "klaw-sync": "^3.0.2",
    "metaed-console": "1.2.1-dev.1",
    "metaed-core": "1.2.1-dev.1",
    "metaed-odsapi-deploy": "1.2.1-dev.1",
    "metaed-plugin-edfi-handbook": "1.2.1-dev.1",
    "metaed-plugin-edfi-interchange-brief": "1.2.1-dev.1",
    "metaed-plugin-edfi-ods": "1.2.1-dev.1",
    "metaed-plugin-edfi-odsapi": "1.2.1-dev.1",
    "metaed-plugin-edfi-sql-dictionary": "1.2.1-dev.1",
    "metaed-plugin-edfi-udm": "1.2.1-dev.1",
    "metaed-plugin-edfi-unified": "1.2.1-dev.1",
    "metaed-plugin-edfi-xml-dictionary": "1.2.1-dev.1",
    "metaed-plugin-edfi-xsd": "1.2.1-dev.1",
    "path": "0.12.7",
    "ramda": "^0.25.0",
    "semver": "^5.5.0",
    "stack-trace": "^0.0.10",
    "stream-splitter": "0.3.2",
    "tmp-promise": "^1.0.4",
    "uuid": "3.0.1",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "mock-fs": "^4.3.0",
    "proxyquire": "^1.8.0"
  },
  "package-deps": [
    "intentions",
    "busy-signal",
    "linter-ui-default",
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  }
}
