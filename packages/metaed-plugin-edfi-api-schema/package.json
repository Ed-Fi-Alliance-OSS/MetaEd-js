  ],
  "metaed-plugin": {
    "pluginType": "artifact-specific",
    "shortName": "edfiApiSchema",
    "authorName": "Ed-Fi Alliance",
    "metaEdVersion": "3.x",
    "technologyVersion": "*",
    "dependencies": [
      "@edfi/metaed-plugin-edfi-unified"
    ]
  },
  "dependencies": {
    "@edfi/metaed-core": "3.2.1-dev.8",
    "@edfi/metaed-plugin-edfi-unified": "3.2.1-dev.8",
