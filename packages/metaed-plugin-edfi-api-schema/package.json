{
  "name": "@edfi/metaed-plugin-edfi-api-schema",
  "main": "dist/index.js",
  "version": "3.2.1-dev.8",
  "description": "MetaEd plugin for generating API Schema object models",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
  "files": [
    "/dist",
