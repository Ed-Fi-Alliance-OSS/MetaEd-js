    "ed-fi-model-3.0": "1.0.0",
    "ed-fi-model-3.1": "1.0.2",
    "final-fs": "^1.6.1"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:dist && yarn build:flow",
