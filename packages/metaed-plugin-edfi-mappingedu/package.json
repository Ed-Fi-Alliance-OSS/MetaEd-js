{
  "name": "metaed-plugin-edfi-mappingedu",
  "main": "dist/index.js",
  "version": "1.4.0",
  "description": "Ed-Fi Alliance MappingEDU plugin for MetaEd",
  "license": "SEE LICENSE IN LICENSE.md",
