{
  "name": "@edfi/metaed-plugin-edfi-mappingedu",
  "main": "dist/index.js",
  "version": "3.0.0-dev.3",
  "description": "Ed-Fi MappingEDU plugin for MetaEd",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "@edfi:registry": "https://pkgs.dev.azure.com/ed-fi-alliance/Ed-Fi-Alliance-Closed/_packaging/edfi-metaed/npm/registry/"
  },
  "files": [
    "/dist",
    "/CHANGELOG.md",
    "/LICENSE.md",
    "/package.json"
  ],
  "metaed-plugin": {
    "pluginType": "artifact-specific",
    "shortName": "edfiMappingedu",
    "authorName": "Ed-Fi Alliance",
    "metaEdVersion": "3.x",
    "technologyVersion": "*",
    "dependencies": [
      "@edfi/metaed-plugin-edfi-unified-advanced",
      "@edfi/metaed-plugin-edfi-xsd"
    ],
    "enabled": false
  },
  "dependencies": {
    "@edfi/metaed-core": "3.0.0-dev.3",
    "@edfi/metaed-plugin-edfi-unified-advanced": "3.0.0-dev.3",
    "@edfi/metaed-plugin-edfi-xsd": "3.0.0-dev.3",
    "xlsx": "^0.14.5"
  },
  "devDependencies": {
    "@edfi/ed-fi-model-2.0": "3.0.0",
    "@edfi/ed-fi-model-2.2": "3.0.0",
    "@edfi/ed-fi-model-3.0": "3.0.0",
    "@edfi/ed-fi-model-3.1": "3.0.0",
    "final-fs": "^1.6.1"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:copy-non-ts && yarn build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "tsc",
    "build:copy-non-ts": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" dist --verbose"
  }
}
