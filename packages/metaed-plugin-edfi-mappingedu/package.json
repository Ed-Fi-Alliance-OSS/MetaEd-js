    "dependencies": [
      "metaed-plugin-edfi-unified",
      "metaed-plugin-edfi-xsd"
    ]
  },
  "dependencies": {
    "metaed-core": "1.4.0",
    "metaed-plugin-edfi-unified": "1.4.0",
