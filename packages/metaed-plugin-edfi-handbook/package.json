{
  "name": "metaed-plugin-edfi-handbook",
  "main": "dist/index.js",
  "version": "2.0.0-dev.14",
  "description": "Ed-Fi MetaEd Handbook plugin for MetaEd",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
