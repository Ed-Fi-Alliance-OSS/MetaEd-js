    "technologyVersion": "*",
    "dependencies": [
      "metaed-plugin-edfi-xsd",
      "metaed-plugin-edfi-ods"
    ]
  },
  "dependencies": {
    "handlebars": "^4.0.10",
    "js-beautify": "^1.7.5",
    "metaed-core": "1.4.2",
    "metaed-plugin-edfi-ods": "1.4.2",
    "metaed-plugin-edfi-unified": "1.4.2",
    "metaed-plugin-edfi-xsd": "1.4.2",
    "ramda": "^0.25.0",
    "sort-array": "^2.0.0",
    "winston": "3.0.0",
    "xlsx": "0.11.19"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:dist && yarn build:flow",
    "build:clean": "rimraf dist",
