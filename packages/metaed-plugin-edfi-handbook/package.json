    "dependencies": [
      "metaed-plugin-edfi-xsd",
      "metaed-plugin-edfi-ods"
    ]
  },
  "dependencies": {
    "handlebars": "^4.0.10",
    "js-beautify": "^1.7.5",
    "metaed-core": "2.0.0-dev.14",
    "metaed-plugin-edfi-ods": "2.0.0-dev.14",
    "metaed-plugin-edfi-unified": "2.0.0-dev.14",
    "metaed-plugin-edfi-xsd": "2.0.0-dev.14",
    "ramda": "^0.25.0",
    "sort-array": "^2.0.0",
    "winston": "3.0.0",
    "xlsx": "0.11.19"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:copy-non-ts && npm run build:dist",
