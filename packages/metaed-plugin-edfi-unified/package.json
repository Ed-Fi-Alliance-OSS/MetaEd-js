{
  "name": "metaed-plugin-edfi-unified",
  "main": "dist/index.js",
  "version": "1.2.2-dev.0",
  "description": "Ed-Fi Alliance unified plugin for MetaEd",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
  "metaed-plugin": {
    "artifact-specific": {
      "shortName": "edfiUnified",
      "authorName": "Ed-Fi Alliance",
      "metaEdVersion": "1.x",
      "technologyVersion": "*",
      "dependencies": []
    }
  },
  "dependencies": {
    "metaed-core": "1.2.2-dev.0",
    "ramda": "^0.25.0",
    "winston": "3.0.0"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:dist && yarn build:flow",
    "build:clean": "rimraf dist",
    "build:flow": "flow-copy-source -v src dist",
    "build:dist": "rimraf dist && babel src -d dist --source-maps inline --copy-files"
  }
