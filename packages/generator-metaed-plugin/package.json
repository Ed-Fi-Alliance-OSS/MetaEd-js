{
  "name": "generator-metaed-plugin",
  "version": "1.4.2",
  "description": "Yeoman generator for MetaEd plugins",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://www.myget.org/F/ed-fi/npm/"
  },
  "files": [
    "/generators",
    "/.npmrc"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^2.1.0",
    "semver": "^5.6.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "scripts": {
    "build": "yarn build:clean",
    "build:clean": "rimraf dist",
    "build:flow": "flow-copy-source -v src dist",
    "build:dist": "rimraf dist && babel src -d dist --source-maps inline --copy-files"
  }
}
