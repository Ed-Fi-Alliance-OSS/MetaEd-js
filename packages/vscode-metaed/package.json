{
	"name": "vscode-metaed",
	"displayName": "MetaEd for VS Code",
	"description": "MetaEd for VS Code",
	"version": "0.0.2-dev.4",
	"publishConfig": {
		"registry": "https://www.myget.org/F/ed-fi/npm/"
	},
	"publisher": "edfi-test",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/client/metaed.js",
	"contributes": {
		"commands": [
			{
				"command": "metaed.build",
				"title": "Build",
				"category": "MetaEd"
			},
			{
				"command": "metaed.about",
				"title": "About",
				"category": "MetaEd"
			}
		],
		"languages": [
			{
				"id": "metaed",
				"extensions": [
					".metaed"
				]
			}
		],
		"grammars": [
			{
				"language": "metaed",
				"scopeName": "source.metaed",
				"path": "./syntaxes/metaed.tmGrammar.json"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "metaed.build",
					"group": "navigation"
				},
				{
					"command": "metaed.about",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn build",
		"build": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn compile",
		"test": "node ./dist/test/runTest.js"
	},
	"dependencies": {
		"ed-fi-model-2.0": "2.0.2",
		"ed-fi-model-2.2": "2.0.2",
		"ed-fi-model-3.0": "2.0.3",
		"ed-fi-model-3.1": "2.0.3",
		"ed-fi-model-3.2a": "2.0.1",
		"fs-extra": "^3.0.1",
		"metaed-console": "0.0.2-dev.4",
		"metaed-core": "0.0.2-dev.4",
		"metaed-odsapi-deploy": "0.0.2-dev.4",
		"metaed-plugin-edfi-handbook": "0.0.2-dev.4",
		"metaed-plugin-edfi-interchange-brief": "0.0.2-dev.4",
		"metaed-plugin-edfi-ods-changequery": "0.0.2-dev.4",
		"metaed-plugin-edfi-ods-postgresql": "0.0.2-dev.4",
		"metaed-plugin-edfi-ods-recordownership": "0.0.2-dev.4",
		"metaed-plugin-edfi-ods-recordownership-postgresql": "0.0.2-dev.4",
		"metaed-plugin-edfi-ods-recordownership-sqlserver": "0.0.2-dev.4",
		"metaed-plugin-edfi-ods-relational": "0.0.2-dev.4",
		"metaed-plugin-edfi-ods-sqlserver": "0.0.2-dev.4",
		"metaed-plugin-edfi-odsapi": "0.0.2-dev.4",
		"metaed-plugin-edfi-sql-dictionary": "0.0.2-dev.4",
		"metaed-plugin-edfi-udm": "0.0.2-dev.4",
		"metaed-plugin-edfi-unified": "0.0.2-dev.4",
		"metaed-plugin-edfi-unified-advanced": "0.0.2-dev.4",
		"metaed-plugin-edfi-xml-dictionary": "0.0.2-dev.4",
		"metaed-plugin-edfi-xsd": "0.0.2-dev.4",
		"ramda": "^0.25.0",
		"semver": "^5.5.0",
		"tmp-promise": "^1.0.4",
		"vscode-languageclient": "^5.2.1",
		"vscode-languageserver": "^5.2.1",
		"vscode-uri": "^2.1.1"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
