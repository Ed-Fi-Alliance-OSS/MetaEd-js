{
	"name": "@edfi/vscode-metaed",
	"displayName": "MetaEd for VS Code",
	"description": "MetaEd for VS Code",
	"license": "SEE LICENSE IN LICENSE.md",
	"version": "3.2.1-dev.0",
	"publishConfig": {
		"registry": "https://www.myget.org/F/ed-fi/npm/"
	},
	"publisher": "edfi-test",
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:metaed"
	],
	"main": "./dist/client/metaed.js",
	"contributes": {
		"commands": [
			{
				"command": "metaed.build",
				"title": "Build",
				"category": "MetaEd"
			},
			{
				"command": "metaed.about",
				"title": "About",
				"category": "MetaEd"
			}
		],
		"languages": [
			{
				"id": "metaed",
				"aliases": [
					"MetaEd",
					"metaed"
				],
				"extensions": [
					".metaed"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "metaed",
				"scopeName": "source.metaed",
				"path": "./syntaxes/metaed.tmLanguage.json"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "metaed.build",
					"group": "navigation"
				},
				{
					"command": "metaed.about",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"build": "yarn build:clean && yarn build:copy-non-ts && yarn build:dist",
		"build:clean": "rimraf dist",
		"build:dist": "tsc",
		"build:copy-non-ts": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" dist --verbose",
		"vscode:prepublish": "yarn build",
		"test": "node ./dist/test/runTest.js"
	},
	"dependencies": {
		"@edfi/ed-fi-model-2.0": "3.0.1",
		"@edfi/ed-fi-model-2.2": "3.0.1",
		"@edfi/ed-fi-model-3.0": "3.0.1",
		"@edfi/ed-fi-model-3.1": "3.0.1",
		"@edfi/ed-fi-model-3.2a": "3.0.1",
		"@edfi/ed-fi-model-3.2b": "3.0.1",
		"@edfi/ed-fi-model-3.2c": "3.0.1",
		"@edfi/ed-fi-model-3.3a": "3.0.1",
		"@edfi/ed-fi-model-3.3b": "3.0.1",
		"@edfi/ed-fi-model-4.0": "3.0.0",
		"@edfi/ed-fi-model-4.0a": "3.0.3",
		"@edfi/metaed-console": "3.2.1-dev.0",
		"@edfi/metaed-core": "3.2.1-dev.0",
		"@edfi/metaed-odsapi-deploy": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-handbook": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-interchange-brief": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-ods-changequery": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-ods-postgresql": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-ods-recordownership": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-ods-recordownership-postgresql": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-ods-recordownership-sqlserver": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-ods-relational": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-ods-sqlserver": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-odsapi": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-sql-dictionary": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-unified": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-unified-advanced": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-xml-dictionary": "3.2.1-dev.0",
		"@edfi/metaed-plugin-edfi-xsd": "3.2.1-dev.0",
		"fs-extra": "^3.0.1",
		"p-debounce": "2.1.0",
		"ramda": "^0.28.0",
		"semver": "^5.7.1",
		"tmp-promise": "^1.1.0",
		"vscode-languageclient": "^8.0.0",
		"vscode-languageserver": "^8.0.0",
		"vscode-uri": "^3.0.6"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/node": "^17.0.45",
		"@types/vscode": "^1.72.0",
		"copyfiles": "^2.4.1",
		"glob": "^7.2.3",
		"mocha": "^9.2.2",
		"rimraf": "^3.0.2",
		"typescript": "^4.8.4",
		"vscode-test": "^1.6.1"
	}
}
