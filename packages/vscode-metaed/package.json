{
	"name": "@edfi/vscode-metaed",
	"displayName": "MetaEd for VS Code",
	"description": "MetaEd for VS Code",
	"version": "3.0.0",
	"publishConfig": {
		"registry": "https://www.myget.org/F/ed-fi/npm/"
	},
	"publisher": "edfi-test",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/client/metaed.js",
	"contributes": {
		"commands": [
			{
				"command": "metaed.build",
				"title": "Build",
				"category": "MetaEd"
			},
			{
				"command": "metaed.about",
				"title": "About",
				"category": "MetaEd"
			}
		],
		"languages": [
			{
				"id": "metaed",
				"extensions": [
					".metaed"
				]
			}
		],
		"grammars": [
			{
				"language": "metaed",
				"scopeName": "source.metaed",
				"path": "./syntaxes/metaed.tmGrammar.json"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "metaed.build",
					"group": "navigation"
				},
				{
					"command": "metaed.about",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn build",
		"build": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn compile",
		"test": "node ./dist/test/runTest.js"
	},
	"dependencies": {
		"@edfi/ed-fi-model-2.0": "3.0.1",
		"@edfi/ed-fi-model-2.2": "3.0.1",
		"@edfi/ed-fi-model-3.0": "3.0.1",
		"@edfi/ed-fi-model-3.1": "3.0.1",
		"@edfi/ed-fi-model-3.2a": "3.0.1",
		"@edfi/metaed-console": "3.0.0",
		"@edfi/metaed-core": "3.0.0",
		"@edfi/metaed-odsapi-deploy": "3.0.0",
		"@edfi/metaed-plugin-edfi-handbook": "3.0.0",
		"@edfi/metaed-plugin-edfi-interchange-brief": "3.0.0",
		"@edfi/metaed-plugin-edfi-ods-changequery": "3.0.0",
		"@edfi/metaed-plugin-edfi-ods-postgresql": "3.0.0",
		"@edfi/metaed-plugin-edfi-ods-recordownership": "3.0.0",
		"@edfi/metaed-plugin-edfi-ods-recordownership-postgresql": "3.0.0",
		"@edfi/metaed-plugin-edfi-ods-recordownership-sqlserver": "3.0.0",
		"@edfi/metaed-plugin-edfi-ods-relational": "3.0.0",
		"@edfi/metaed-plugin-edfi-ods-sqlserver": "3.0.0",
		"@edfi/metaed-plugin-edfi-odsapi": "3.0.0",
		"@edfi/metaed-plugin-edfi-sql-dictionary": "3.0.0",
		"@edfi/metaed-plugin-edfi-unified": "3.0.0",
		"@edfi/metaed-plugin-edfi-unified-advanced": "3.0.0",
		"@edfi/metaed-plugin-edfi-xml-dictionary": "3.0.0",
		"@edfi/metaed-plugin-edfi-xsd": "3.0.0",
		"fs-extra": "^3.0.1",
		"ramda": "^0.28.0",
		"semver": "^5.7.1",
		"tmp-promise": "^1.1.0",
		"vscode-languageclient": "^7.0.0",
		"vscode-languageserver": "^7.0.0",
		"vscode-languageserver-textdocument": "^1.0.4",
		"vscode-uri": "^3.0.3"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/node": "^17.0.14",
		"@types/vscode": "^1.63.2",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"vscode-test": "^1.6.1"
	}
}
