Domain Entity ProjectDimension
    documentation "NCES Project Dimension"
    shared string Code named ProjectCode
        documentation "The code representation of the account dimension."
        is part of identity
    shared short MyFiscalYear named FiscalYear
        documentation "The fiscal year for which the account dimension is valid."
        is part of identity
    shared string CodeName
        documentation "A description of the account dimension."
        is optional
    //Optional Collection
    descriptor ReportingTag
        documentation "Optional tag for accountability reporting (e.g. ESSA)."
        is optional collection
