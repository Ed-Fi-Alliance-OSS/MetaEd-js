  },
  "scripts": {
    "prepublishOnly": "npm run build && npm test && npm run lint",
    "test": "mocha --compilers js:babel-register \"test/**/*Tests.js\" \"test-integration/**/*Tests.js\"",
    "build": "babel src -d dist --source-maps",
    "lint": "npm run flow && npm run eslint && npm run eslint:tests",
    "flow": "flow check",
    "eslint": "eslint src",
    "eslint:tests": "eslint test && eslint test-integration",
