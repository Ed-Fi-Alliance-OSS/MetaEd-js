    "final-fs": "1.6.1",
    "handlebars": "^4.0.10",
    "immutable": "3.8.1",
    "ramda": "^0.23.0",
    "safety-lens": "1.5.0",
    "winston": "2.3.1",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.11.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.46.0",
    "flow-typed": "^2.1.2",
    "mocha": "^2.5.3",
    "mock-fs": "^4.3.0",
    "rimraf": "^2.6.1",
    "standard-version": "^4.0.0"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register \"test/**/*Tests.js\" \"test-integration/**/*Tests.js\"",
    "build": "rimraf dist/ && babel src -d dist --source-maps",
    "lint": "yarn run flow && yarn run eslint && yarn run eslint:tests",
    "flow": "flow check",
    "eslint": "eslint src",
    "eslint:tests": "eslint test && eslint test-integration",
    "release:version-dev": "yarn run build && yarn test && yarn run lint && standard-version --prerelease dev",
    "release:push-tags": "git push --follow-tags",
    "release:publish-dev": "yarn run release:push-tags && npm publish --tag=dev"
  }
}
